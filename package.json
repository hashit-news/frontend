{
  "name": "frontendv2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "compile": "tsc --noEmit --pretty",
    "prettier:check": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
    "prettier:fix": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "prepare": "husky install",
    "test:e2e": "playwright test",
    "codegen": "openapi -i ./src/api/backend/swagger.json -o ./src/api/backend/swagger -c axios --name BackendApiClient --useUnionTypes --useOptions"
  },
  "dependencies": {
    "@headlessui/react": "^1.6.6",
    "@heroicons/react": "^1.0.6",
    "@metamask/detect-provider": "^1.2.0",
    "@metamask/providers": "^9.0.0",
    "@types/axios": "^0.14.0",
    "axios": "^0.27.2",
    "jwt-decode": "^3.1.2",
    "moment": "^2.29.4",
    "next": "12.2.3",
    "next-themes": "^0.2.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "web3": "^1.7.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.24.0",
    "@tailwindcss/typography": "^0.5.4",
    "@types/moment": "^2.13.0",
    "@types/node": "18.6.1",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "autoprefixer": "^10.4.7",
    "daisyui": "^2.20.0",
    "eslint": "^8.20.0",
    "eslint-config-next": "12.2.3",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "openapi-typescript-codegen": "^0.23.0",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.6",
    "typescript": "4.7.4"
  }
}
